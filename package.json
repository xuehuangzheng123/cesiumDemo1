{
	"name": "natural-resources-platform",
	"private": true,
	"version": "1.1.0",
	"description": "Natural Resources Rights determination and registration platform",
	"author": "liuhuitao",
	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"build:dev": "vue-tsc --noEmit && vite build --mode development",
		"build:test": "vue-tsc --noEmit && vite build --mode test",
		"build:pro": "vue-tsc --noEmit && vite build --mode production",
		"build": "vite build --mode production",
		"preview": "vite preview",
		"lint:eslint": "eslint --fix --ext .js,.ts,.vue,.tsx ./src",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md,tsx}\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:lint-staged": "lint-staged",
		"prepare": "husky install",
		"release": "standard-version",
		"commit": "git pull && git add -A && git-cz && git push",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
	},
	"dependencies": {
		"@iconify/json": "^2.1.146",
		"@potato/antd": "^1.0.15",
		"@vueuse/core": "^8.0.1",
		"ant-design-vue": "^3.2.15",
		"axios": "^1.2.2",
		"crypto-js": "^4.1.1",
		"echarts": "^5.3.0",
		"echarts-liquidfill": "^3.1.0",
		"js-cookie": "^3.0.1",
		"js-md5": "^0.7.3",
		"lodash": "^4.17.21",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.12",
		"pinia-plugin-persistedstate": "^1.6.1",
		"print-js": "^1.6.0",
		"qs": "^6.11.0",
		"terraformer-wkt-parser": "^1.2.1",
		"tinycolor2": "^1.6.0",
		"vue": "^3.2.41",
		"vue-i18n": "^9.1.9",
		"vue-request": "^2.0.0-rc.4",
		"vue-router": "^4.0.12",
		"vue3-seamless-scroll": "^1.2.0",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.1",
		"@commitlint/config-conventional": "^17.0.0",
		"@types/jsdom": "^20.0.1",
		"@types/lodash": "^4.14.191",
		"@types/node": "^17.0.31",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@unocss/preset-attributify": "^0.47.4",
		"@unocss/preset-icons": "^0.47.4",
		"@unocss/preset-uno": "^0.47.4",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"autoprefixer": "^10.4.7",
		"commitizen": "^4.2.4",
		"cz-git": "^1.3.2",
		"driver.js": "^0.9.8",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-vue": "^8.7.1",
		"husky": "^8.0.1",
		"less": "^4.1.3",
		"lint-staged": "^12.4.2",
		"postcss": "^8.4.14",
		"postcss-html": "^1.4.1",
		"prettier": "^2.6.2",
		"rollup-plugin-visualizer": "^5.5.4",
		"standard-version": "^9.5.0",
		"stylelint": "^14.8.5",
		"stylelint-config-html": "^1.0.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-property-sort-order-smacss": "^9.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^25.0.0",
		"stylelint-order": "^5.0.0",
		"typescript": "^4.9.4",
		"unocss": "^0.47.4",
		"unplugin-auto-import": "^0.6.0",
		"unplugin-vue-components": "^0.17.18",
		"vite": "^3.2.3",
		"vite-plugin-cdn-import": "^0.3.5",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.6.0",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue-tsc": "^0.40.4"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	}
}
